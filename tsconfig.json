{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths":{
      "@components/*": ["./src/components/*"],
      "@Authentication/*": ["src/screens/Authentication/*"],
      "@screens/*": ["./src/screens/*"],
      "@assets/*": ["./src/assets/*"],
      "@theme/*": ["./src/theme/*"],
      "@hooks/*": ["./src/hooks/*"],
    },
}
}
